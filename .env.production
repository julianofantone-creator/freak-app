# Production Frontend Environment Variables
# Deploy these to Netlify environment settings

VITE_API_URL=https://freaky-api.railway.app
VITE_WS_URL=wss://freaky-api.railway.app
VITE_APP_NAME=Freaky
VITE_ENVIRONMENT=production

# Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CRASH_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Social sharing
VITE_APP_URL=https://freaky.app
VITE_SUPPORT_EMAIL=support@freaky.app

# Performance
VITE_CDN_URL=https://cdn.freaky.app
VITE_ENABLE_SERVICE_WORKER=true